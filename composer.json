{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "torres-developer/pull",
    "description": "An PHP cURL extension wrapper",
    "type": "library",
    "keywords": [
        "curl",
        "libcurl",
        "fiber"
    ],
    "homepage": "https://github.com/torres-developer/php-pull",
    "readme": "./README.md",
    "time": "2023-01-07",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "torres-developer",
            "email": "torres.dev@disroot.org",
            "homepage": "https://github.com/torres-developer",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/torres-developer/php-pull/issues",
        "source": "https://github.com/torres-developer/php-pull/"
    },
    "require": {
        "php": ">=8.1.6",
        "pds/skeleton": "^1.0",
        "ext-curl": "*",
        "torres-developer/http-message-impl": "dev-master",
        "react/promise": "3.x-dev",
        "react/async": "4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "TorresDeveloper\\Pull\\": "src/"
        },
        "files": [
            "src/pull.php"
        ]
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/torres-developer/php-pull"
        }
    ]
}
